{
  "name": "Export CrazyDomains DNS",
  "description": "Easily export DNS records from CrazyDomains in a format ready to import to GoDaddy. *FreeParking has been rebranded to CrazyDomains in New Zealand.*",
  "version": "0.2.0",
  "manifest_version": 3,
  "icons": {
    "16": "icons/icon16.png",
    "32": "icons/icon32.png",
    "48": "icons/icon48.png",
    "128": "icons/icon128.png"
  },
  "content_scripts": [
    {
      "matches": [
        "https://manage.freeparking.co.nz/members/domains/details/*",
        "https://www.crazydomains.co.nz/members*"
      ],
      "run_at": "document_end",
      "js": ["/scripts/export.js"]
    }
  ]
}
